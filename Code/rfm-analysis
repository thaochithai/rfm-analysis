        "#segment counts and monetary sums\n",
        "segment_counts = frm_cal['Segment'].value_counts()\n",
        "segment_value = frm_cal.groupby('Segment')['Monetary'].sum()\n",
        "\n",
        "#percentage contributions\n",
        "segment_counts_pct = (segment_counts / segment_counts.sum()) * 100\n",
        "segment_value_pct = (segment_value / segment_value.sum()) * 100\n",
        "\n",
        "# Combine data into a DataFrame for plotting\n",
        "plot_data = pd.DataFrame({\n",
        "    'Segment Counts (%)': segment_counts_pct,\n",
        "    'Segment Value (%)': segment_value_pct\n",
        "})\n",
        "\n",
        "#plot bar\n",
        "fig, ax = plt.subplots(figsize=(10, 6))\n",
        "bar_width = 0.35\n",
        "indices = range(len(plot_data))\n",
        "ax.bar([i - bar_width / 2 for i in indices], plot_data['Segment Counts (%)'], bar_width, label='Counts (%)', color='black')\n",
        "ax.bar([i + bar_width / 2 for i in indices], plot_data['Segment Value (%)'], bar_width, label='Monetary Value (%)', color='orange')\n",
        "\n",
        "#labels and title\n",
        "ax.set_xlabel('Segment')\n",
        "ax.set_ylabel('% Contribution')\n",
        "ax.set_title('Percentage Contribution of Segment Counts and Monetary Value')\n",
        "ax.set_xticks(indices)\n",
        "ax.set_xticklabels(plot_data.index, rotation=45)\n",
        "ax.legend()\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()"
