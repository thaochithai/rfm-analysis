        "# Set the visual style\n",
        "plt.style.use('ggplot')\n",
        "sns.set(style=\"whitegrid\")\n",
        "\n",
        "def plot_rfm_distribution(frm_cal):\n",
        "    \"\"\"Plot the distribution of R, F, and M values\"\"\"\n",
        "    fig, axes = plt.subplots(1, 3, figsize=(18, 6))\n",
        "\n",
        "    # R distribution\n",
        "    sns.histplot(frm_cal['Recency'], bins=20, kde=True, ax=axes[0], color='#ff7f0e')\n",
        "    axes[0].set_title('Recency Distribution', fontsize=14)\n",
        "    axes[0].set_xlabel('Days since last purchase', fontsize=12)\n",
        "    axes[0].set_ylabel('Count', fontsize=12)\n",
        "\n",
        "    # F distribution\n",
        "    sns.histplot(frm_cal['Frequency'], bins=20, kde=True, ax=axes[1], color='#2ca02c')\n",
        "    axes[1].set_title('Frequency Distribution', fontsize=14)\n",
        "    axes[1].set_xlabel('Number of purchases', fontsize=12)\n",
        "    axes[1].set_ylabel('Count', fontsize=12)\n",
        "\n",
        "    # M distribution\n",
        "    sns.histplot(frm_cal['Monetary'], bins=20, kde=True, ax=axes[2], color='#d62728')\n",
        "    axes[2].set_title('Monetary Distribution', fontsize=14)\n",
        "    axes[2].set_xlabel('Total spend', fontsize=12)\n",
        "    axes[2].set_ylabel('Count', fontsize=12)\n",
        "\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "plot_rfm_distribution(frm_cal)"
