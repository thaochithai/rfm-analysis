      "cell_type": "code",
      "source": [
        "# Average basket size by segment\n",
        "def calculate_segment_statistics(frm_cal):\n",
        "    # Group by segment and calculate statistics\n",
        "    segment_stats = frm_cal.groupby('Segment').agg({\n",
        "        'CustomerID': 'count',\n",
        "        'Monetary': 'sum'\n",
        "    }).reset_index()\n",
        "    # Calculate derived metrics\n",
        "    segment_stats['Avg_Value_Per_Customer'] = segment_stats['Monetary'] / segment_stats['CustomerID']\n",
        "    # Rename columns for clarity\n",
        "    segment_stats.rename(columns={'CustomerID': 'Customer_Count'}, inplace=True)\n",
        "    return segment_stats\n",
        "# After completing your RFM analysis\n",
        "segment_stats_df = calculate_segment_statistics(frm_cal)\n",
        "print(segment_stats_df)"
      ],
