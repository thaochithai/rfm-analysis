      ]
    },
    {
      "cell_type": "markdown",
      "source": [
        "2.3. Grading base on quantile"
      ],
      "metadata": {
        "id": "SCdXVn3CL1wb"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "#grade RFM metrics based on quantiles\n",
        "frm_cal['R'] = pd.qcut(frm_cal['Recency'], 5, labels=[5,4,3,2,1])\n",
        "frm_cal['F'] = frm_cal['Frequency'].rank(ascending=True, method='first')\n",
        "frm_cal['F'] = pd.qcut(frm_cal['F'], 5, labels=[1, 2, 3, 4, 5])\n",
        "frm_cal['M'] = pd.qcut(frm_cal['Monetary'], 5, labels=[1, 2, 3, 4, 5])\n",
        "frm_cal['RFM'] = frm_cal['R'].astype(str)+ frm_cal['F'].astype(str)+ frm_cal['M'].astype(str)\n",
        "print(frm_cal)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "UpuYrcWkL6jP",
        "outputId": "291835ab-4ffe-4312-9075-3467923bb2d9"
