      "cell_type": "code",
      "source": [
        "#remove rows with null values\n",
        "data_cleaned = data.dropna(subset=['InvoiceNo', 'CustomerID', 'Quantity', 'UnitPrice'])\n",
        "\n",
        "#remove duplicate rows\n",
        "data_cleaned = data_cleaned.drop_duplicates()\n",
        "\n",
        "#remove rows where Quantity or UnitPrice is negative\n",
        "data_cleaned = data_cleaned[(data_cleaned['Quantity'] > 0) & (data_cleaned['UnitPrice'] > 0)]\n",
        "\n",
        "data_cleaned['CustomerID'] = data_cleaned['CustomerID'].astype(int)\n",
        "\n",
        "print(data_cleaned.describe())"
      ],
