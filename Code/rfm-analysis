      "cell_type": "code",
      "source": [
        "#Bubble chart\n",
        "def plot_segment_bubble(frm_cal):\n",
        "    \"\"\"Create a bubble chart showing the relationship between segments\"\"\"\n",
        "    # Calculate aggregates by segment\n",
        "    segment_summary = frm_cal.groupby('Segment').agg({\n",
        "        'CustomerID': 'count',\n",
        "        'Recency': 'mean',\n",
        "        'Frequency': 'mean',\n",
        "        'Monetary': 'mean'\n",
        "    }).reset_index()\n",
        "\n",
        "    # Rename columns for clarity\n",
        "    segment_summary.rename(columns={'CustomerID': 'Count'}, inplace=True)\n",
        "\n",
        "    # Calculate total monetary value for each segment\n",
        "    segment_summary['Total_Value'] = segment_summary['Count'] * segment_summary['Monetary']\n",
        "\n",
        "    # Calculate percentage of total value\n",
        "    total_value = segment_summary['Total_Value'].sum()\n",
        "    segment_summary['Value_Percentage'] = (segment_summary['Total_Value'] / total_value) * 100\n",
        "\n",
        "    # Create the bubble chart\n",
        "    plt.figure(figsize=(14, 10))\n",
        "\n",
        "    # Create a colormap for the segments\n",
        "    cmap = plt.cm.viridis\n",
        "    colors = cmap(np.linspace(0, 1, len(segment_summary)))\n",
        "\n",
        "    # Create the scatter plot with bubble size representing total value\n",
        "    scatter = plt.scatter(\n",
        "        segment_summary['Recency'],\n",
        "        segment_summary['Frequency'],\n",
        "        s=segment_summary['Value_Percentage'] * 100,  # Scale bubble size\n",
        "        c=segment_summary['Monetary'],\n",
        "        cmap='viridis',\n",
        "        alpha=0.7,\n",
        "        edgecolors='w',\n",
        "        linewidths=1\n",
        "    )\n",
        "\n",
        "    # Add segment labels to each bubble\n",
        "    for i, row in segment_summary.iterrows():\n",
        "        plt.annotate(\n",
        "            row['Segment'],\n",
        "            xy=(row['Recency'], row['Frequency']),\n",
        "            xytext=(5, 0),\n",
        "            textcoords='offset points',\n",
        "            ha='center',\n",
        "            va='center',\n",
        "            fontsize=11,\n",
        "            fontweight='bold',\n",
        "            color='black'\n",
        "        )\n",
        "\n",
        "    # Add a color bar to show the monetary value scale\n",
        "    plt.colorbar(scatter, label='Average Monetary Value')\n",
        "\n",
        "    # Add axis labels and title\n",
        "    plt.xlabel('Average Recency (days since last purchase)', fontsize=12)\n",
        "    plt.ylabel('Average Frequency (number of purchases)', fontsize=12)\n",
        "    plt.title('Customer Segments: Recency vs Frequency vs Monetary Value', fontsize=14)\n",
        "\n",
        "    # Add a legend for the bubble size\n",
        "    sizes = [5, 10, 20]\n",
        "    labels = ['5%', '10%', '20%']\n",
        "    leg = plt.legend(\n",
        "        handles=[plt.scatter([], [], s=s*100, edgecolors='none', color='gray') for s in sizes],\n",
        "        labels=labels,\n",
        "        title=\"% of Total Value\",\n",
        "        loc=\"upper right\",\n",
        "        frameon=True,\n",
        "        labelspacing=2,\n",
        "        title_fontsize=12\n",
        "    )\n",
        "\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "\n",
        "plot_segment_bubble(frm_cal)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
