      "cell_type": "code",
      "source": [
        "def plot_heatmap_correlation(frm_cal):\n",
        "    \"\"\"Create a heatmap of correlations between RFM metrics\"\"\"\n",
        "    # Select the numeric columns for correlation\n",
        "    numeric_data = frm_cal[['Recency', 'Frequency', 'Monetary', 'R', 'F', 'M']]\n",
        "\n",
        "    # Calculate the correlation matrix\n",
        "    corr = numeric_data.corr()\n",
        "\n",
        "    # Create a mask for the upper triangle\n",
        "    mask = np.triu(np.ones_like(corr, dtype=bool))\n",
        "\n",
        "    # Create the heatmap\n",
        "    plt.figure(figsize=(10, 8))\n",
        "\n",
        "    # Generate a custom diverging colormap\n",
        "    cmap = sns.diverging_palette(230, 20, as_cmap=True)\n",
        "\n",
        "    # Draw the heatmap\n",
        "    sns.heatmap(\n",
        "        corr,\n",
        "        mask=mask,\n",
        "        cmap=cmap,\n",
        "        vmax=1,\n",
        "        vmin=-1,\n",
        "        center=0,\n",
        "        square=True,\n",
        "        linewidths=.5,\n",
        "        cbar_kws={\"shrink\": .5},\n",
        "        annot=True\n",
        "    )\n",
        "\n",
        "    plt.title('Correlation Between RFM Metrics', fontsize=16)\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "\n",
        "plot_heatmap_correlation(frm_cal)"
      ],
